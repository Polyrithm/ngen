{
  "version": "0.2.0",
  "configurations": [


    {
      "type": "node",
      "request": "launch",
      "name": "init",
      "preLaunchTask": "typescript",
      "runtimeVersion": "8.12.0",
      "program": "${workspaceFolder}/bin/run",
      "protocol": "auto",
      "trace": true,
      "console": "integratedTerminal",
      "smartStep": true,
      "sourceMaps": true,
      "outputCapture": "console",
      "args": [
        "init"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "is:online",
      "preLaunchTask": "typescript",
      "runtimeVersion": "8.12.0",
      "program": "${workspaceFolder}/bin/run",
      "protocol": "auto",
      "trace": true,
      "console": "integratedTerminal",
      "smartStep": true,
      "sourceMaps": true,
      "outputCapture": "console",
      "args": [
        "is:online"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "make:component",
      "preLaunchTask": "typescript",
      "runtimeVersion": "8.12.0",
      "program": "${workspaceFolder}/bin/run",
      "protocol": "auto",
      "trace": true,
      "console": "integratedTerminal",
      "smartStep": true,
      "sourceMaps": true,
      "outputCapture": "console",
      "args": [
        "make:component",
        "HelloWorld"
      ]
    },

    {
      "type": "node",
      "request": "launch",
      "name": "generate:interceptor",
      "preLaunchTask": "typescript",
      "runtimeVersion": "8.12.0",
      // "preLaunchTask": "typescript",
      "program": "${workspaceFolder}/bin/run",
      "protocol": "auto",
      "cwd": "${workspaceRoot}",
      "internalConsoleOptions": "openOnSessionStart",
      "trace": true,
      "console": "integratedTerminal",
      "smartStep": true,
      "sourceMaps": true,
      "outputCapture": "console",
      "args": [
        "g:i",
        "test"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Mocha Tests",
      "preLaunchTask": "typescript",
      "program": "${workspaceFolder}/node_modules/mocha/bin/_mocha",
      "args": [
        "-u",
        "tdd",
        "--timeout",
        "999999",
        "--colors",
        "${workspaceFolder}/test/**/*.test.ts"
      ],
      "internalConsoleOptions": "openOnSessionStart"
    }
  ]
}
