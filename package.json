{
  "name": "@polyrithm/ngen",
  "description": "CLI Utility tool for AngularJS Development",
  "version": "0.0.3",
  "author": "Nivrith",
  "bin": {
    "ngen": "./bin/run"
  },
  "bugs": "https://github.com/Polyrithm/ngen/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "@types/chalk": "^2.2.0",
    "@types/handlebars": "^4.0.39",
    "chalk": "^2.4.1",
    "handlebars": "^4.0.12",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@oclif/tslint": "^3",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "chai": "^4",
    "globby": "^8",
    "gulp": "^3.9.1",
    "mocha": "^5",
    "nyc": "^13",
    "ts-node": "^7",
    "tslint": "^5",
    "typescript": "^3.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/src/templates",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/Polyrithm/ngen",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "ngen",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "macos": {
      "identifier": "com.ngen.cli"
    }
  },
  "repository": "Polyrithm/ngen",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json npm-shrinkwrap.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc && gulp copy:templates && oclif-dev manifest && oclif-dev readme && npm shrinkwrap",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
